    border 5px solid black
    border-radius 10px  
    margin-left:-100px
    margin-top:80px

#display
    width 200
    height 40px
    text-align center
    background-color black
    border 3px solid white
    font-size 18px
    left 2px
    top 2px
    color #7fff00
    border-radius 10px
    margin-left 40px

.btnTop
    background-color #6f6f6f

.btnNum
    background-color black

.btnMath
    background-color #6f6f6f

.btnOpps
    background-color #6f6f6f
    
.btnTop,.btnNum,.btnMath,.btnOpps
